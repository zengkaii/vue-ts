.icon-btn {
  display: inline-block;
  line-height: 1;
  white-space: nowrap;
  cursor: pointer;
  background: #fff;
  border: 1px solid #d8dce5;
  color: #5a5e66;
  -webkit-appearance: none;
  text-align: center;
  outline: none;
  margin: 0;
  transition: 0.1s;
  font-weight: 500;
  padding: 5px;
  border-radius: 4px;
}
.icon-btn:hover {
  border-color: #5a5e66;
}
.icon-btn.danger {
  color: #fa5555;
  background: #fee;
  border-color: #fdbbbb;
}
.icon-btn.danger:hover {
  border-color: #fa5555;
}
.icon-btn.primary {
  color: #fff;
  background: #20A0FF;
  border-color: #20A0FF;
}
.icon-btn.primary:hover {
  border-color: #6dc1ff;
  background: #6dc1ff;
}
.icon-btn.success {
  color: #fff;
  background: #13CE66;
  border-color: #13CE66;
}
.icon-btn.success:hover {
  border-color: #40ed8d;
  background: #40ed8d;
}
.icon-btn + .icon-btn {
  margin-left: 5px;
}
.clickable {
  cursor: pointer;
}
